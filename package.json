{
  "name": "mern-app",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run react-dev\"",
    "server": "cd server && npm start",
    "react-dev": "cd client && npm start",
    "test": "cd server && npm test",
    "install": "cd client && npm install",
    "installServer": "cd server && npm install",
    "installAll": "concurrently \"npm run install\" \"npm run installServer\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "concurrently \"npm run installAll\" \"npm run build\""
  },
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0"
  }
}
